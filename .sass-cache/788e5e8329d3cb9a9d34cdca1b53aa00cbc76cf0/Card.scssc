$Red_Pinline: #C23D34;
$Red_Bar: #F9D5C0;
$Red_Text: #F7E6DE;
$Green_Card: #7ea17e;
$Green_Pinline: #483;
$Green_Bar: #cdc;
$Green_Text: #DEEADE;
