.Red .cardtitlebar, .Red .cardtypebar { background-color: $Red_Bar; }
.Red .cardtext, .Red .pinline_ptbox { background: $Red_Text !important; }

.Green.card, .Green .cardart { background-color: $Green_Card; /* #6b7 */}
