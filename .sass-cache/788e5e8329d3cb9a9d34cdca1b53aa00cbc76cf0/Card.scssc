    border-left: none; }
  /* Right side inner */
  .pinline_sides > div, .pinline_sidesbottom  > div {
    border-right: 1px solid black; }
  /* Right side outer */
  .pinline_sides, .pinline_sidesbottom {
    border-right: none; }
  /* Bottom inner */
